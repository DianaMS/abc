{
  "name": "yevo-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --watch=false",
    "test-headless": "ng test --browsers=ChromeHeadless --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run yevo-front:serve-ssr",
    "serve:ssr": "node dist/yevo-front/server/main.js",
    "build:ssr": "ng build --prod --outputHashing=all && ng run yevo-front:server:production",
    "build:dev:ssr": "ng build --outputHashing=all && ng run yevo-front:server",
    "prerender": "ng run yevo-front:prerender",
    "generate:svg-sprite": "svg2sprite ./src/assets/icons ./src/assets/sprites/sprite.svg --stripAttrs fill --stripAttrs stroke --stripAttrs id",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/yevo-front/browser/stats.json",
    "bundle-source-map": "ng build --prod --source-map true && source-map-explorer dist/yevo-front/browser/*.js",
    "imageoptim": "imageoptim --imagealpha 'src/assets/**/*.png'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.5",
    "@angular/cdk": "^12.2.0",
    "@angular/common": "~10.1.5",
    "@angular/compiler": "~10.1.5",
    "@angular/core": "~10.1.5",
    "@angular/fire": "^6.0.4",
    "@angular/forms": "~10.1.5",
    "@angular/google-maps": "^12.2.6",
    "@angular/localize": "^10.2.2",
    "@angular/platform-browser": "~10.1.5",
    "@angular/platform-browser-dynamic": "~10.1.5",
    "@angular/platform-server": "~10.1.5",
    "@angular/router": "~10.1.5",
    "@angular/service-worker": "~10.1.5",
    "@mailchimp/mailchimp_transactional": "^1.0.50",
    "@ng-bootstrap/ng-bootstrap": "^8.0.0",
    "@ng-select/ng-select": "^5.0.9",
    "@ngqp/core": "^1.1.0",
    "@nguniversal/express-engine": "^10.1.0",
    "@ngx-lite/json-ld": "^0.6.3",
    "@sentry/angular": "^6.19.3",
    "@sentry/browser": "^6.19.3",
    "@sentry/node": "^6.19.3",
    "@sentry/tracing": "^6.19.3",
    "@types/mailchimp__mailchimp_transactional": "^1.0.5",
    "@types/marked": "^5.0.0",
    "angular-calendar": "^0.28.28",
    "angular-svg-icon": "^10.0.0",
    "angularx-flatpickr": "^6.6.0",
