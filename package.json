    "dayjs": "^1.10.3",
    "dom-parser": "^0.1.6",
    "express": "^4.15.2",
    "firebase": "^8.9.1",
    "flatpickr": "^4.6.9",
    "glob-parent": "^5.1.2",
    "i": "^0.3.7",
    "jquery": "^3.5.1",
    "jspdf": "^2.5.1",
    "localstorage-polyfill": "^1.0.1",
    "lodash": "^4.17.21",
    "marked": "^5.1.0",
    "ng-lazyload-image": "^9.1.0",
